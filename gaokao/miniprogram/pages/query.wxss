}

.nodata{
  text-align: center;
  width: 100%;
  font-size: 18px;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #2eecee
}

/* 返回顶部 */
.goTop{
  height: 100rpx;
  width: 100rpx;
  position: fixed;
  z-index: 10;
  bottom: 70rpx;
  background: #2eecee;
  right: 50rpx;
  border-radius: 50%;
}


/* dialog start */

.dialog_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 10001;
  color: #fff;
}

.dialog_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 200001;
  background: #fff;
  padding-top: 1px;
}

.dialog_title {
  font-size: 16px;
  height: 30px;
  display: flex;
  align-items: center;
  padding: 10px;
  background: #2eecee;
  color: white;
}

.dialog_content {
  position: relative;
  float: left;
  padding: 10px 10px;
  width: 25%;
  box-sizing: border-box;
}

/* dialog end */

.type_pre{
  display: flex;
  text-align: center;
  margin: 8px;
  align-items: center;
  justify-content: center;
  height: 30px;
  background: #2eecee
}

.type_nor{
  display: flex;
  text-align: center;
  margin: 8px;
  justify-content: center;
  align-items: center;
  height: 30px;
}

ad{
  z-index: 10000;
}

ad1{
  position: absolute;
  z-index: 10000;
}
